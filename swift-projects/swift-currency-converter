var pesos: Double = 10.0
var reais: Double = 5.0
var soles: Double = 10.0
var total: Double = 0.0
// 1 peso is 0.00027 USD
// 1 reais is 0.19 USD
// 1 sol is 0.28 USD
var rate1: Double = 0.00027
var rate2: Double = 0.19
var rate3: Double = 0.28

total = rate1 * pesos + rate2 * reais + rate3 * soles
print("US Dollars = $\(total)")
